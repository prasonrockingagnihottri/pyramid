#include <stdio.h>
void
main ()
{
  int i, j, k, m, n;


  printf ("enter the no of rows :");
  scanf ("%d", &n);
  m = n - 4 + 1;

  for (i = 1; i <= n; i++)
    {
      for (k = m; k >= 1; k--)
	{
	  printf (" ");

	}
      for (j = 1; j <= i; j++)

	printf ("%d ", i);


      printf ("\n");
      m--;
    }
}
